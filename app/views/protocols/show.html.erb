  <article class="module width_full">
	  <header><h3><%= I18n.t :showing %></h3></header>
    <div class="tab_container">
		  <div id="tab1" class="tab_content">
			  <table class="tablesorter" cellspacing="0">
          <tr> 
            <td><b><%= I18n.t :id %></b></th>
            <td><%= @protocol.id %></td>            
			    </tr>
          <tr> 
            <td><b><%= I18n.t :id %></b></th>
            <td><%= @protocol.name %></td>            
			    </tr>
          <tr> 
            <td><b><%= I18n.t :id %></b></th>
            <td><%= @protocol.enabled %></td>            
			    </tr>
        </table>
      </div>
    </div>

    <footer>
      <div class="submit_link">
        <%= link_to image_tag("/assets/icn_edit_article.png", :alt => I18n.t(:edit), :title => I18n.t(:edit)), edit_protocol_path(@protocol) %> 
        <%= link_to image_tag("/assets/icn_jump_back.png", :alt => I18n.t(:back), :title => I18n.t(:back)), protocols_path %>
      </div>
    </footer>
  </article>
